{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "astro/client",
      "@types/react",
      "@types/node"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ]
    }
  },
  "include": [
    ".astro/types.d.ts",
    "src",
    "**/*.astro",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "dist"
  ]
}